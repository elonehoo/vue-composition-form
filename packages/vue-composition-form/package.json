{
  "name": "vue-composition-form",
  "type": "module",
  "version": "0.0.0",
  "description": "Vue Composition API for validating form.",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "keywords": [
    "composition",
    "vue",
    "form"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
