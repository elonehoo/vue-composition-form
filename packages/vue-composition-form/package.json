    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts"
  },
  "peerDependencies": {
    "vue": ">=3.2.45"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.setwith": "^4.3.2",
    "lodash.unset": "^4.5.2"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.setwith": "^4.3.7",
    "@types/lodash.unset": "^4.5.7"
  }
}
